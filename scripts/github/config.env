# scripts/github/config.env
export REPO="https://github.com/jlmayorgaco/py-power-systems-frequency-estimator-open-bench"          # <-- set this to your GitHub repo
export PROJECT_NUMBER=""          # optional (GitHub Projects v2)
export DRY_RUN=1                  # 1 = dry-run; 0 = live

# DEFAULT_BRANCH auto-detects; override only if you want:
export DEFAULT_BRANCH="${DEFAULT_BRANCH:-$(git rev-parse --abbrev-ref HEAD 2>/dev/null || echo main)}"
[[ "$DEFAULT_BRANCH" = "HEAD" || -z "$DEFAULT_BRANCH" ]] && DEFAULT_BRANCH="main"
